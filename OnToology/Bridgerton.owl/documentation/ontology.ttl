#################################################################

###  http://www.semanticweb.org/UOC-Raul_Oscar/ontologies/2022/2/Bridgerton#Belongs_to_family
:Belongs_to_family rdf:type owl:ObjectProperty ;
                   rdfs:domain :Nobility ;
                   rdfs:range :Families_Nobility ;
                   rdfs:comment "A nobility character has a family" ;
                   rdfs:label "Belongs to family"@en .


###  http://www.semanticweb.org/UOC-Raul_Oscar/ontologies/2022/2/Bridgerton#Is_brother_or_sister_of
:Is_brother_or_sister_of rdf:type owl:ObjectProperty ;
                         rdfs:domain :Characters ;
                         rdfs:range :Characters ;
                         rdfs:comment "Link with brother of sisters" ;
                         rdfs:label "Is brother or sister of"@en .


###  http://www.semanticweb.org/UOC-Raul_Oscar/ontologies/2022/2/Bridgerton#Is_dependant_of
:Is_dependant_of rdf:type owl:ObjectProperty ;
                 rdfs:domain :Characters ;
                 rdfs:range :Characters ;
                 rdfs:comment "The character of who depends" ;
                 rdfs:label "Is dependant of"@en .


###  http://www.semanticweb.org/UOC-Raul_Oscar/ontologies/2022/2/Bridgerton#Is_distributed_by
:Is_distributed_by rdf:type owl:ObjectProperty ;
                   rdfs:domain :Seassons ;
                   rdfs:range :Content_Delivery_Platforms ;
                   rdfs:comment "Seasons are distributed by a company" ;
                   rdfs:label "Is distributed by"@en .


###  http://www.semanticweb.org/UOC-Raul_Oscar/ontologies/2022/2/Bridgerton#Is_father_of
:Is_father_of rdf:type owl:ObjectProperty ;
              rdfs:domain :Nobility ;
              rdfs:range :Nobility ;
              rdfs:comment "Property to determine the father" ;
              rdfs:label "Is father of"@en .


###  http://www.semanticweb.org/UOC-Raul_Oscar/ontologies/2022/2/Bridgerton#Plays_the_role_of
:Plays_the_role_of rdf:type owl:ObjectProperty ;
                   rdfs:domain :Filming_Crew ;
                   rdfs:range :Characters ;
                   rdfs:comment "An actor/Actress plays the role of an especific character." ;
                   rdfs:label "Plays the role of"@en .


